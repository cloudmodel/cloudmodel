<%- if service.graphite_web_enabled -%>
location /graphite/ {
        gzip off;
        include uwsgi_params;
        uwsgi_pass      127.0.0.1:3031;
}
location /content {
        alias /usr/lib/python2.7/site-packages/graphite/content;
        gzip off;
        include uwsgi_params;
        uwsgi_pass      127.0.0.1:3031;
}
location /metrics {
        alias /usr/lib/python2.7/site-packages/graphite/metrics;
        gzip off;
        include uwsgi_params;
        uwsgi_pass      127.0.0.1:3031;
}
location /dashboard {
        alias /usr/lib/python2.7/site-packages/graphite/dashboard;
        gzip off;
        include uwsgi_params;
        uwsgi_pass      127.0.0.1:3031;
}
location /render {
        alias /usr/lib/python2.7/site-packages/graphite/render;
        gzip off;
        include uwsgi_params;
        uwsgi_pass      127.0.0.1:3031;
}
location /browser {
        alias /usr/lib/python2.7/site-packages/graphite/browser;
        gzip off;
        include uwsgi_params;
        uwsgi_pass      127.0.0.1:3031;
}
location /composer {
        alias /usr/lib/python2.7/site-packages/graphite/composer;
        gzip off;
        include uwsgi_params;
        uwsgi_pass      127.0.0.1:3031;
}

<%- else service.graphite_web_enabled -%>
location /render {
        alias /usr/lib/python2.7/site-packages/graphite/render;
        gzip off;
        include uwsgi_params;
        uwsgi_pass      127.0.0.1:3031;
}

<%- end -%>