location /graphite/ {
  gzip off;
  root /usr/lib/python2.7/dist-packages/graphite/public;
  passenger_user _graphite;
  passenger_group _graphite;
  passenger_enabled on;
}
<%- if service.graphite_web_enabled -%>
location /content {
  passenger_user _graphite;
  passenger_group _graphite;
  alias /usr/lib/python2.7/dist-packages/graphite/content;
  gzip off;
}
location /metrics {
  passenger_user _graphite;
  passenger_group _graphite;
  alias /usr/lib/python2.7/dist-packages/graphite/metrics;
  gzip off;
}
location /dashboard {
  passenger_user _graphite;
  passenger_group _graphite;
  alias /usr/lib/python2.7/dist-packages/graphite/dashboard;
  gzip off;
}
location /browser {
  passenger_user _graphite;
  passenger_group _graphite;
  alias /usr/lib/python2.7/dist-packages/graphite/browser;
  gzip off;
}
location /composer {
  passenger_user _graphite;
  passenger_group _graphite;
  alias /usr/lib/python2.7/dist-packages/graphite/composer;
  gzip off;
}
<%- end -%>
location /render {
  passenger_user _graphite;
  passenger_group _graphite;
  alias /usr/lib/python2.7/dist-packages/graphite/render;
  gzip off;
}
