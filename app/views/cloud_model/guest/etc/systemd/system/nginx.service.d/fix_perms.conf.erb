[Service]
PermissionsStartOnly=true
ExecStartPre=/bin/mkdir -p /var/log/nginx
ExecStartPre=/bin/chown -R www:www /var/log/nginx
ExecStartPre=/bin/chmod 2770 /var/log/nginx

ExecStartPre=/bin/mkdir -p /var/www/rails/current
ExecStartPre=/bin/chmod 2770 /var/www/rails
ExecStartPre=/bin/chmod 2770 /var/www/rails/current
ExecStartPre=/bin/chown -R www:www /var/www
ExecStartPre=-/usr/bin/sudo -u www RAILS_ENV=<%= model.passenger_env %> /var/www/rails/current/script/prestart